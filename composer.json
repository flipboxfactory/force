{
  "name": "flipboxfactory/force",
  "description": "Salesforce Integration for Craft CMS",
  "version": "1.0.0-rc.1",
  "type": "craft-plugin",
  "license": "proprietary",
  "keywords": [
    "cms",
    "craftcms",
    "salesforce",
    "force.com",
    "yii2"
  ],
  "require": {
    "craftcms/cms": "^3.0.14",
    "flipboxfactory/patron": "^1.0.0-rc.4",
    "stevenmaguire/oauth2-salesforce": "^2.0",
    "flipboxdigital/relay-salesforce": "~2.0",
    "flipboxfactory/craft-integration": "^1.1",
    "flipboxfactory/craft-psr16": "^1.0",
    "flipboxfactory/craft-psr3": "^1.0",
    "flipboxfactory/flux": "~1.0.0-rc.5",
    "flipboxdigital/pipeline": "^1.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2",
    "codeception/codeception": "^2.4",
    "vlucas/phpdotenv": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\force\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\force\\tests\\": "tests/src/"
    }
  },
  "support": {
    "email": "support@flipboxfactory.com",
    "issues": "https://github.com/flipboxfactory/force/issues?state=open",
    "source": "https://github.com/flipboxfactory/force",
    "docs": "https://github.com/flipboxfactory/force",
    "rss": "https://github.com/flipboxfactory/force/commits/master.atom"
  },
  "extra": {
    "name": "Salesforce",
    "handle": "force",
    "class": "flipbox\\force\\Force",
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/force/master/CHANGELOG.md",
    "downloadUrl": "https://github.com/flipboxfactory/force/archive/master.zip",
    "hasCpSection": true,
    "hasCpSettings": true,
    "schemaVersion": "1.1.0"
  }
}