{
  "name": "flipboxfactory/force",
  "description": "Salesforce Integration",
  "version": "1.0.0-beta.3",
  "type": "craft-plugin",
  "license": "proprietary",
  "keywords": [
    "cms",
    "craftcms",
    "salesforce",
    "force.com",
    "yii2"
  ],
  "require": {
    "craftcms/cms": "^3.0.0",
    "flipboxdigital/salesforce": "^2.1.0",
    "flipboxfactory/craft-ember": "^1.0.0",
    "flipboxfactory/craft-psr16": "^1.0.0",
    "flipboxfactory/craft-psr3": "^1.0.0",
    "flipboxfactory/flux": "~1.0.0-rc",
    "stevenmaguire/oauth2-salesforce": "^2.0",
    "flipboxfactory/craft-sortable-associations": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\force\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\force\\tests\\": "tests/src/"
    }
  },
  "support": {
    "email": "support@flipboxfactory.com",
    "issues": "https://github.com/flipboxfactory/force/issues?state=open",
    "source": "https://github.com/flipboxfactory/force",
    "docs": "https://github.com/flipboxfactory/force",
    "rss": "https://github.com/flipboxfactory/force/commits/master.atom"
  },
  "extra": {
    "name": "Salesforce",
    "handle": "force",
    "class": "flipbox\\force\\Force",
    "hasCpSection": true,
    "hasCpSettings": true,
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/force/master/CHANGELOG.md",
    "downloadUrl": "https://github.com/flipboxfactory/force/archive/master.zip",
    "components": {
      "cache": "flipbox\\force\\services\\Cache",
      "connections": "flipbox\\force\\services\\Connections",
      "elements": "flipbox\\force\\services\\Elements",
      "logger": "flipbox\\craft\\psr3\\Logger",
      "queryField": "flipbox\\force\\services\\QueryField",
      "queries": "flipbox\\force\\services\\Queries",
      "resources": "flipbox\\force\\services\\Resources",
      "sObjectAssociations": "flipbox\\force\\services\\SObjectAssociations",
      "sObjectsField": "flipbox\\force\\services\\SObjectsField",
      "transformers": "flipbox\\force\\services\\Transformers"
    },
    "modules": {
      "cp": {
        "basePath": "@flipbox/force/cp",
        "class": "flipbox\\force\\cp\\Cp",
        "components": {
          "queryManager": "flipbox\\force\\cp\\services\\QueryManager"
        }
      }
    }
  }
}